{
  "name": "cv-website",
  "version": "1.0.0",
  "description": "CV website",
  "main": "webpack.config.js",
  "scripts": {
    "start": "rm -rf dist && webpack -w",
    "build": "rm -rf dist && webpack -p --optimize-dedupe",
    "dev": "node_modules/.bin/webpack-dev-server --content-base dist/",
    "deploy": "git push origin :gh-pages && git subtree push --prefix dist origin gh-pages"
  },
  "keywords": [
    "cv",
    "html",
    "web"
  ],
  "author": "Martin Othamar",
  "license": "MIT",
  "devDependencies": {
    "css-loader": "^0.26.0",
    "extract-loader": "0.0.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "image-webpack-loader": "^3.0.0",
    "sass-loader": "^4.1.1",
    "style-ext-html-webpack-plugin": "^3.0.8",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3"
  },
  "dependencies": {
    "node-sass": "^4.1.1",
    "webpack-dev-server": "^1.16.2"
  }
}
